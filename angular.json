{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "popup": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/popup",
      "sourceRoot": "projects/popup/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "outputPath": {
              "base": "dist/popup"
            },
            "index": "projects/popup/src/index.html",
            "tsConfig": "projects/popup/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/popup/src/favicon.ico",
              "projects/popup/src/assets"
            ],
            "styles": ["projects/popup/src/styles.scss"],
            "stylePreprocessorOptions": {
              "includePaths": ["node_modules/"]
            },
            "scripts": ["out-tsc/inject.js"],
            "optimization": {
              "styles": {
                "inlineCritical": false
              }
            },
            "browser": "projects/popup/src/main.ts"
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "none"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "development"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "polyfills": [],
            "tsConfig": "projects/popup/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/popup/src/favicon.ico",
              "projects/popup/src/assets",
              {
                "glob": "**/*",
                "input": "projects/popup/src/app/testdata",
                "output": "/assets/"
              }
            ],
            "styles": ["projects/popup/src/styles.scss"],
            "scripts": [],
            "karmaConfig": "projects/popup/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/popup/**/*.ts",
              "projects/popup/**/*.html"
            ]
          }
        }
      }
    },
    "options": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/options",
      "sourceRoot": "projects/options/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "outputPath": {
              "base": "dist/options"
            },
            "index": "projects/options/src/index.html",
            "tsConfig": "projects/options/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/options/src/favicon.ico",
              "projects/options/src/assets"
            ],
            "styles": ["projects/options/src/styles.scss"],
            "stylePreprocessorOptions": {
              "includePaths": ["node_modules/"]
            },
            "optimization": {
              "styles": {
                "inlineCritical": false
              }
            },
            "scripts": [],
            "allowedCommonJsDependencies": ["highlight.js", "sanitize-html"],
            "browser": "projects/options/src/main.ts"
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "none"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "development"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "polyfills": [],
            "tsConfig": "projects/options/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/options/src/favicon.ico",
              "projects/options/src/assets"
            ],
            "styles": ["projects/options/src/styles.scss"],
            "scripts": [],
            "karmaConfig": "projects/options/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/options/**/*.ts",
              "projects/options/**/*.html"
            ]
          }
        }
      }
    },
    "common": {
      "projectType": "library",
      "root": "projects/common",
      "sourceRoot": "projects/common/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "options": {
            "project": "projects/common/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/common/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/common/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "development"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "tsConfig": "projects/common/tsconfig.spec.json",
            "polyfills": [],
            "karmaConfig": "projects/common/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/common/**/*.ts",
              "projects/common/**/*.html"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "370f6e11-d0c3-462f-8d45-c34e97c038f9",
    "schematicCollections": ["@angular-eslint/schematics"]
  },
  "schematics": {
    "@schematics/angular:component": {
      "type": "component"
    },
    "@schematics/angular:directive": {
      "type": "directive"
    },
    "@schematics/angular:service": {
      "type": "service"
    },
    "@schematics/angular:guard": {
      "typeSeparator": "."
    },
    "@schematics/angular:interceptor": {
      "typeSeparator": "."
    },
    "@schematics/angular:module": {
      "typeSeparator": "."
    },
    "@schematics/angular:pipe": {
      "typeSeparator": "."
    },
    "@schematics/angular:resolver": {
      "typeSeparator": "."
    }
  }
}
