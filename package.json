{
  "name": "test3",
  "version": "0.0.0",
  "lint-staged": {
    "*.{js,scss,md,html,json,ts}": "prettier --write",
    "*.{html,ts}": "eslint --cache --fix",
    "*.scss": "stylelint --fix"
  },
  "scripts": {
    "ng": "ng",
    "build": "rm -rf dist/ && ng build common && ng build popup && ng build options && cp manifest.json dist/",
    "release": "rm -rf dist/ && rm -f dist.zip && ng build common --configuration=production && ng build popup --configuration=production && ng build options --configuration=production && cp manifest.json dist/ && cd dist && zip -r ../dist.zip . * && cd ../",
    "test": "ng test --no-watch --no-progress --browsers=ChromeHeadless",
    "lint": "ng lint && npm run lint:styles",
    "lint:styles": "stylelint --fix \"projects/**/*.scss\"",
    "coverage": "ng test --no-watch --code-coverage"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.0.0",
    "@angular/cdk": "^18.0.0",
    "@angular/common": "^18.0.0",
    "@angular/compiler": "^18.0.0",
    "@angular/core": "^18.0.0",
    "@angular/forms": "^18.0.0",
    "@angular/material": "^18.0.0",
    "@angular/platform-browser": "^18.0.0",
    "@angular/platform-browser-dynamic": "^18.0.0",
    "@angular/router": "^18.0.0",
    "highlight.js": "^11.9.0",
    "ngx-highlightjs": "^10.0.0",
    "rxjs": "~7.8.0",
    "sanitize-html": "^2.13.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.0.1",
    "@angular-eslint/builder": "17.5.1",
    "@angular-eslint/eslint-plugin": "17.5.1",
    "@angular-eslint/eslint-plugin-template": "17.5.1",
    "@angular-eslint/template-parser": "17.5.1",
    "@angular/cli": "^18.0.1",
    "@angular/compiler-cli": "^18.0.0",
    "@types/chrome": "^0.0.263",
    "@types/jasmine": "~5.1.0",
    "@types/sanitize-html": "^2.11.0",
    "@typescript-eslint/eslint-plugin": "7.2.0",
    "@typescript-eslint/parser": "7.2.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^18.0.0",
    "prettier": "^3.2.5",
    "stylelint": "^16.3.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "typescript": "~5.4.2"
  }
}
